-- Scripts:

local function PSNPLWW_fake_script() -- Headshot.LocalScript 
	local script = Instance.new('LocalScript', Headshot)

	script.Parent.Image =  game.Players:GetUserThumbnailAsync(game.Players.LocalPlayer.UserId, "HeadShot", Enum.ThumbnailSize.Size100x100)
end
coroutine.wrap(PSNPLWW_fake_script)()
local function UKSF_fake_script() -- Username.LocalScript 
	local script = Instance.new('LocalScript', Username)

	script.Parent.Text = "Hello, "..game.Players.LocalPlayer.DisplayName
end
coroutine.wrap(UKSF_fake_script)()
local function FKOW_fake_script() -- Flavour.LocalScript 
	local script = Instance.new('LocalScript', Flavour)

	script.Parent.Text = "Enjoying your "..os.date("%A").."?"
end
coroutine.wrap(FKOW_fake_script)()
local function IBDIG_fake_script() -- FacT.LocalScript 
	local script = Instance.new('LocalScript', FacT)

	local n = math.random(0,3)
	if n== 0 then 
		script.Parent.Text = "Did you know? Oxide was created all the way back in 2022!"
	elseif n==1 then 
		script.Parent.Text = "Fun Fact: Hemi is pretty cool"
	elseif n == 2 then
		script.Parent.Text = "Fun Fact: Oxide is called Oxide because Hemi used to go by Nitro. Confusing, right?"
	elseif n == 3 then
		script.Parent.Text = "Fun Fact: Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua"
	end
end
coroutine.wrap(IBDIG_fake_script)()
